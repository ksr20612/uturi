{
  "name": "@uturi/sonification",
  "version": "2.2.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "description": "Accessibility tool that converts numerical data into musical melodies for visually impaired users",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "keywords": [
    "accessibility",
    "a11y",
    "sonification",
    "music",
    "melody"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "dev:test": "npx http-server . -p 3000 -c-1",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "vue": "^3.0.0",
    "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@uturi/config-eslint": "workspace:*",
    "@uturi/config-typescript": "workspace:*",
    "@uturi/config-vitest": "workspace:*",
    "svelte": "^4.0.0",
    "standardized-audio-context-mock": "^9.7.24",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.0.0",
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksr20612/uturi.git",
    "directory": "packages/sonification"
  },
  "homepage": "https://github.com/ksr20612/uturi/tree/main/packages/sonification#readme",
  "bugs": {
    "url": "https://github.com/ksr20612/uturi/issues"
  }
}
