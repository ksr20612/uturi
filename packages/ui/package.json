{
  "name": "@uturi/ui",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Design system library with accessible React components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "design-system",
    "react",
    "components",
    "ui",
    "accessibility",
    "a11y"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "dev:test": "npx http-server . -p 3001 -c-1",
    "storybook": "storybook dev -p 6006",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@uturi/config-eslint": "workspace:*",
    "@uturi/config-typescript": "workspace:*",
    "@uturi/config-vitest": "workspace:*",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "csstype": "^3.1.3",
    "eslint-plugin-storybook": "9.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^9.1.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksr20612/uturi.git",
    "directory": "packages/ui"
  },
  "homepage": "https://github.com/ksr20612/uturi/tree/main/packages/ui#readme",
  "bugs": {
    "url": "https://github.com/ksr20612/uturi/issues"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/recipes": "^0.5.7"
  }
}
