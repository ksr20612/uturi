{
  "name": "@uturi/maru",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "FSM-based headless design system for building accessible UI components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "keywords": [
    "fsm",
    "state-machine",
    "headless",
    "design-system",
    "accessible",
    "a11y"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "xstate": "^5.24.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@uturi/config-eslint": "workspace:*",
    "@uturi/config-typescript": "workspace:*",
    "@uturi/config-vitest": "workspace:*",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksr20612/uturi.git",
    "directory": "packages/maru"
  },
  "homepage": "https://github.com/ksr20612/uturi/tree/main/packages/maru#readme",
  "bugs": {
    "url": "https://github.com/ksr20612/uturi/issues"
  }
}
